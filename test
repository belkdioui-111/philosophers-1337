  if(philos[i].deid)
        {
            j++;
        }
        if(j == philos[i].args->n_of_philo)
        {
            break;
        }
        if(get_curr_time()-philos[i].last_eat >= philos[i].args->t_to_die)
        {
             printf("%lld %d is died\n", get_curr_time() - philos[i].args->f_time, philos[i].id + 1);
             break;
        }
        i++;
        if(philos->args->n_of_philo == i)
        {
            i = 0;
        }